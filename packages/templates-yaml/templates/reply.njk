---
date: {{ published }}
in-reply-to: {{ inReplyTo }}
{%- if rsvp %}
rsvp: {{ rsvp }}
{%- endif %}
{%- if category %}
category:
{%- for item in category %}
- {{ item }}
{%- endfor %}
{%- endif %}
{%- if syndicateTo %}
syndicate-to:
{%- for item in syndicateTo %}
- {{ item }}
{%- endfor %}
{%- endif %}
---
{{ content | safe }}
