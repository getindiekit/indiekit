{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Indiekit",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@indiekit/indiekit/bin/cli.js",
      // "args": ["--config", "path/to/your/indiekit.config.js"]
      "args": ["serve"],
      "env": {
        "GITHUB_TOKEN": "${env:GITHUB_TOKEN}",
        "MONGO_URL": "${env:MONGO_URL}"
      }
    }
  ]
}
